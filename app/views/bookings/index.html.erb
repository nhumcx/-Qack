
<div class='container'>
 <h1>My Quaks:</h1>
<% raise %>
  <div class="card-product">
    <% @bookings.each do |booking| %>
    <div class="col-xs-12 col-md-4">
      <div class="card-product-infos" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(<%= cl_image_path booking.duck.photo.key, width: 200, crop: :fill %>)">
        <%= link_to "", booking_path(booking) %>
          <div>
            <h3><%= booking.status %>:</h3>
          </div>
       </div>
      </div>
       <% end %>
  </div>
</div>
